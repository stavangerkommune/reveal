{
  "manifest_version": 3,
  "name": "reveal",
  "version": "0.0.2",
  "description": "Dan 2:22. Viser en knapp som tar deg fra Visma-ordre til Famac-aktivitet",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://*/ehandel/ehandel?*",
        "https://*/*/ehandel/ehandel/*/?*"
      ]
    }
  ]
}